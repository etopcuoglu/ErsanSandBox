    <validationRules>
        <fullName>Cannot_Self_reference</fullName>
        <active>true</active>
        <description>Validation rule to prevent an Account Relationship From and To the same Account.</description>
        <errorConditionFormula>AND($Profile.Name = &quot;AE User&quot;, From_Account__c = To_Account__c)</errorConditionFormula>
        <errorMessage>You cannot create a Relationship from an Account to itself.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_2_business_accounts</fullName>
        <active>true</active>
        <description>You cannot create an Account Relationship between 2 non-person accounts</description>
        <errorConditionFormula>AND(
NOT(From_Account__r.IsPersonAccount),
NOT(To_Account__r.IsPersonAccount)
)</errorConditionFormula>
        <errorMessage>You cannot create an Account Relationship between two business-type Accounts</errorMessage>
    </validationRules>
